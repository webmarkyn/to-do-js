{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": ["airbnb"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
      "no-shadow": "off",
      "no-param-reassign": "off",
      "eol-last": "off",
      "no-underscore-dangle": "off",
      "no-alert": "off",
      "no-restricted-globals": "off",
      "import/no-cycle": "off",
      "no-use-before-define": "warn"
    }
}